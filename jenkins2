pipeline {
   agent any
   tools {
        maven 'maven' 
    }
    
    stages {
        stage('clone') {
            steps {
               git branch: 'main', url: 'https://github.com/knbtechnosys/JAVA_Project_6pm.git'
            }
        }
        
        stage('build') {
            steps {
                sh 'mvn install'
            }
        }
    }
}
